  "devDependencies": {
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^2.0.7",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.6.0",
    "helmet": "^6.0.0",
